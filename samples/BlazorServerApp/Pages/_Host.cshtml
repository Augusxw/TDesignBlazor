@page "/"
@namespace BlazorServerApp.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>TDesignBlazorServerApp</title>
    <base href="~/" />
    <link rel="stylesheet" href="css/bootstrap/bootstrap.min.css" />
    <link href="css/site.css" rel="stylesheet" />
    <link href="BlazorServerApp.styles.css" rel="stylesheet" />
    @*    <link rel="stylesheet" href="https://unpkg.com/tdesign-vue/dist/tdesign.min.css" />
    <link rel="stylesheet" href="https://tdesign.gtimg.com/icon/0.1.2/fonts/index.css" />*@
    <link href="~/tdesign-vue@0.46.4/dist/tdesign.min.css" rel="stylesheet" />
    <link href="~/tesign.gtimg.com/icon/0.1.2/fonts/index.css" rel="stylesheet" />
    
    <script src="~/tesign.gtimg.com/icon/0.1.2/fonts/index.js"></script>
    <script src="https://github.com/Tencent/tdesign-vue-next/blob/develop/src/hooks/useRipple.ts"></script>
</head>
<body>
    <component type="typeof(App)" render-mode="ServerPrerendered" />

    <div id="blazor-error-ui">
        <environment include="Staging,Production">
            An error has occurred. This application may no longer respond until reloaded.
        </environment>
        <environment include="Development">
            An unhandled exception has occurred. See browser dev tools for details.
        </environment>
        <a href="" class="reload">Reload</a>
        <a class="dismiss">🗙</a>
    </div>
    @*https://tdesign.gtimg.com/icon/0.1.2/fonts/t.woff*@
    <script src="_framework/blazor.server.js"></script>
    @*    <script src="https://unpkg.com/tdesign-vue/dist/tdesign.min.js"></script>
    <script src="https://tdesign.gtimg.com/icon/0.1.2/fonts/index.js"></script>*@
</body>
</html>
